{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "prettier",
    "jest",
    "@typescript-eslint"
  ],
  "rules": {
    "prettier/prettier": "error",
    "comma-dangle": "off",
    "space-before-function-paren": "off",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": ["error", { "argsIgnorePattern": "_" }],
    "multiline-ternary": "off",
    "camelcase": "off"
  }
}
